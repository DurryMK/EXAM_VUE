\t\t\t<el-container>\r\n\t\t\t\t<el-main>\r\n\t\t\t\t\t<el-tabs v-model=\"editableTabsValue\" type=\"card\" editable @edit=\"handleTabsEdit\" @tab-remove=\"handleTabsClose\">\r\n\t\t\t\t\t\t<el-tab-pane :key=\"item.name\" v-for=\"(item, index) in editableTabs\" :label=\"item.title\" :name=\"item.name\">\r\n\t\t\t\t\t\t\t{{item.content}}\r\n\t\t\t\t\t\t</el-tab-pane>\r\n\t\t\t\t\t</el-tabs>\r\n\t\t\t\t\t<Make v-show=\"editableTabsValue == 101\" />\r\n\t\t\t\t</el-main>\r\n\t\t\t\t<!-- 底部 -->\r\n\t\t\t\t<el-footer>\r\n\t\t\t\t\t<div>\r\n\t\t\t\t\t\t<el-divider></el-divider>\r\n\t\t\t\t\t\t<span>@copy Durry</span>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</el-footer>\r\n\t\t\t</el-container>\r\n\t\t</el-container>\r\n\t</el-container>\r\n</template>\r\n\r\n<script>\r\n\timport Make from \"./Make.vue\"\r\n\tlet tabSet = new Set()\r\n\texport default {\r\n\t\tname: 'home',\r\n\t\tprops: {\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t\tMake\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tvisible: false,\r\n\t\t\t\tloading: true,\r\n\t\t\t\tisCollapse: true,\r\n\t\t\t\tactiveIndex: '1',\r\n\t\t\t\tactiveIndex2: '1',\r\n\t\t\t\tisCollapse: true,\r\n\t\t\t\tfullscreenLoading: false,\r\n\t\t\t\teditableTabsValue: '1',\r\n\t\t\t\teditableTabs: [{\r\n\t\t\t\t\ttitle: '欢迎',\r\n\t\t\t\t\tname: '1',\r\n\t\t\t\t\tcontent: '欢迎使用EXAM考试平台',\r\n\t\t\t\t\tclosable: false,\r\n\t\t\t\t}],\r\n\t\t\t\ttabIndex: 1\r\n\t\t\t};\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\tinitPage() {\r\n\t\t\t\tthis.fullscreenLoading = true\r\n\t\t\t},\r\n\t\t\thandleOpen(key, keyPath) {\r\n\t\t\t\tconsole.log(key, keyPath);\r\n\t\t\t},\r\n\t\t\thandleClose(key, keyPath) {\r\n\t\t\t\tconsole.log(key, keyPath);\r\n\t\t\t},\r\n\t\t\thandleSelect(key, keyPath) {\r\n\t\t\t\tconsole.log(key, keyPath);\r\n\t\t\t},\r\n\t\t\thandleSelectAside(data) {\r\n\t\t\t\tconsole.log(data);\r\n\t\t\t},\r\n\t\t\t//选中侧边导航栏的 切换标签页\r\n\t\t\tcilekItem(data) {\r\n\t\t\t\tvar index = data.$vnode.componentOptions.propsData[\"index\"]\r\n\t\t\t\tif (tabSet.has(index)) {\r\n\t\t\t\t\tthis.editableTabsValue = index\r\n\t\t\t\t\treturn\r\n\t\t\t\t}\r\n\t\t\t\ttabSet.add(index)\r\n\t\t\t\tvar title = data.$vnode.componentOptions.children[0].text\r\n\t\t\t\tconsole.log(data)\r\n\t\t\t\tthis.editableTabs.push({\r\n\t\t\t\t\ttitle: title,\r\n\t\t\t\t\tname: index,\r\n\t\t\t\t});\r\n\t\t\t\tthis.editableTabsValue = index\r\n\t\t\t},\r\n\t\t\thandleTabsClose(data) {\r\n\t\t\t\ttabSet.delete(data)\r\n\t\t\t},\r\n\t\t\thandleTabsEdit(targetName, action) {\r\n\t\t\t\tif (action === 'add') {\r\n\t\t\t\t\tlet newTabName = ++this.tabIndex + '';\r\n\t\t\t\t\tthis.editableTabs.push({\r\n\t\t\t\t\t\ttitle: 'New Tab',\r\n\t\t\t\t\t\tname: newTabName,\r\n\t\t\t\t\t\tcontent: 'New Tab content'\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.editableTabsValue = newTabName;\r\n\t\t\t\t}\r\n\t\t\t\tif (action === 'remove') {\r\n\t\t\t\t\tlet tabs = this.editableTabs;\r\n\t\t\t\t\tlet activeName = this.editableTabsValue;\r\n\t\t\t\t\tif (activeName === targetName) {\r\n\t\t\t\t\t\ttabs.forEach((tab, index) => {\r\n\t\t\t\t\t\t\tif (tab.name === targetName) {\r\n\t\t\t\t\t\t\t\tlet nextTab = tabs[index + 1] || tabs[index - 1];\r\n\t\t\t\t\t\t\t\tif (nextTab) {\r\n\t\t\t\t\t\t\t\t\tactiveName = nextTab.name;\r\n\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t}\r\n\r\n\t\t\t\t\tthis.editableTabsValue = activeName;\r\n\t\t\t\t\tthis.editableTabs = tabs.filter(tab => tab.name !== targetName);\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n</style>\n"]}]}